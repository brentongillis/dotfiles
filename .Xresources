URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,fullscreen
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.F11:           perl:fullscreen:switch

!! Appearance
urxvt.termName: rxvt-unicode
urxvt.scrollBar: false
urxvt.background: #2d2d2d
!!urxvt.background: #2f343f
urxvt.foreground: #E8FFFD

!! Adjust Rxvt to suit your terminal emulator
*color0: #393939
*color1: #ca674a
*color2: #96a967
*color3: #d3a94a
*color4: #5778c1
*color5: #9c35ac
*color6: #6eb5f3
*color7: #a9a9a9
*color8: #535551
*color9: #ea2828
*color10: #87dd32
*color11: #f7e44d
*color12: #6f9bca
*color13: #a97ca4
*color14: #32dddd
*color15: #e9e9e7

!! Font preferenes
urxvt.font: xft:Liberation Mono:size=11
urxvt.boldFont: xft:Liberation Mono:size=11:weight=bold
!!urxvt.letterSpace: -1
